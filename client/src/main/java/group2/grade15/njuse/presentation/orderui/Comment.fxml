<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane fx:id="rootNode" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
          prefHeight="500.0" prefWidth="500.0" style="-fx-background-color: white;"
          xmlns="http://javafx.com/javafx/8.0.72" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="group2.grade15.njuse.presentation.orderui.CommentController">
    <columnConstraints>
        <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" prefWidth="150.0"/>
        <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" minWidth="10.0" prefWidth="150.0"/>
        <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" prefWidth="150.0"/>
        <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" minWidth="10.0" prefWidth="150.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="200.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Label text="酒店名称：" GridPane.rowIndex="2"/>
        <Label fx:id="hotelNameLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <Label text="评分：" GridPane.rowIndex="3"/>
        <Label text="我的评价" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.halignment="CENTER">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
        <Label fx:id="cancelLabel" onMouseClicked="#close" GridPane.rowIndex="5"/>
        <Label text="订单号：" GridPane.rowIndex="1"/>
        <Label text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <Label fx:id="confirmLabel" GridPane.columnIndex="3" GridPane.rowIndex="5"/>
        <Label text="订单生成时间：" GridPane.columnIndex="2" GridPane.rowIndex="1"/>
        <Label text="Label" GridPane.columnIndex="3" GridPane.rowIndex="1"/>
        <Label fx:id="editLabel" GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.rowIndex="4"/>
        <ChoiceBox prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
        <Label text="评论：" GridPane.rowIndex="4"/>
        <TextArea prefHeight="200.0" prefWidth="200.0" wrapText="true" GridPane.columnIndex="1" GridPane.columnSpan="2"
                  GridPane.rowIndex="4"/>
    </children>
    <effect>
        <DropShadow/>
    </effect>
</GridPane>
